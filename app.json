      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "resize",
      "adaptiveIcon": {
        "foregroundImage": "./assets/branding_logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.user1.abdulrehman.luminamood"
    },
    "web": {
      "favicon": "./assets/branding_logo.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "86effc55-2b37-4a5a-a0ba-fd2f00e08913"
      }
    },
    "owner": "user1.abdulrehman"
  }
