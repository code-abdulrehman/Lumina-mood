{
  "expo": {
    "name": "Lumina Mood",
    "slug": "lumina-mood",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/branding_logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
